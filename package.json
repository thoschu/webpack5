{
  "name": "webpack5",
  "version": "1.0.0",
  "description": "https://webpack.js.org/",
  "main": "",
  "scripts": {
    "webpack": "npx webpack --config webpack.config.js",
    "webpack:development": "npx webpack --mode=development",
    "webpack:production": "npx webpack --mode=production",
    "webpack:serve": "npx webpack serve",
    "webpack:serve:development": "npx webpack serve --mode=development",
    "webpack:serve:production": "npx webpack serve --mode=production",
    "webpack:stats": "npx webpack --stats detailed",
    "serve": "npx http-server -o ./dist --log-ip true",
    "serve:all": "npx concurrently \"npm run serve:hello\" \"npm run serve:car\"",
    "serve:hello": "npx http-server -o ./dist/hello.html --log-ip true",
    "serve:car": "npx http-server -o ./dist/car.html --log-ip true",
    "server": "node server.js",
    "clean": "npx rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tom S.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-proposal-record-and-tuple": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@bloomberg/record-tuple-polyfill": "^0.0.4",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "react": "^18.3.1"
  }
}
