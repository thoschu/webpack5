{
  "name": "webpack5_dashboard",
  "version": "1.0.0",
  "description": "POC: https://webpack.js.org/",
  "main": "",
  "scripts": {
    "webpack": "npx webpack --config webpack.config.js",
    "webpack:development": "npx webpack --mode=development",
    "webpack:production": "npx webpack --mode=production",
    "webpack:serve": "npx webpack serve --hot",
    "webpack:serve:development": "npx webpack serve --mode=development",
    "webpack:serve:production": "npx webpack serve --mode=production",
    "webpack:stats": "npx webpack --stats detailed",
    "serve": "npx http-server -o ./dist --log-ip true",
    "serve:dashboard": "npx http-server --port 8844 -o ./dist/dashboard.html --log-ip true",
    "server:dashboard": "node server.js",
    "clean:dist": "npx rimraf dist",
    "clean:node_modules": "npx rimraf node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tom S.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "express": "^4.19.2"
  }
}
